		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0"
	},
	"build": {
		"asar": true,
		"productName": "ProsperAIO",
		"appId": "com.prosperaio.prosperaio",
		"copyright": "Copyright Â© 2019 ProsperAIO",
		"mac": {
			"category": "public.app-category.utilities",
			"target": [
				{
					"target": "DMG"
				}
			],
			"icon": "assets/img/prosper-icon.icns",
			"type": "distribution"
		},
		"dmg": {
			"backgroundColor": "#1f1e1e",
			"iconSize": 80,
			"iconTextSize": 12,
			"title": "${productName} ${version}"
		},
		"win": {
			"target": [
				{
					"target": "NSIS"
				}
			],
			"icon": "assets/img/prosper-icon.ico"
		},
		"nsis": {
			"oneClick": true,
			"perMachine": false,
			"installerIcon": "assets/img/prosperaio-ico.ico",
			"installerHeaderIcon": "",
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"shortcutName": "ProsperAIO"
		},
		"files": [
			"build/**",
			"assets/**"
		]
	}
}
