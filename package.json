		"font-awesome": "^4.7.0",
		"got": "^11.5.1",
		"jquery": "^3.5.1",
		"jsencrypt": "^3.0.0-rc.1",
		"menu": "^0.2.5",
		"moment": "^2.27.0",
		"mousetrap": "^1.6.3",
		"node-machine-id": "^1.1.10",
		"ora": "^5.0.0",
		"os": "^0.1.1",
		"puppeteer-core": "^3.3.0",
		"puppeteer-extra": "^3.1.13",
		"puppeteer-extra-plugin-stealth": "^2.4.15",
		"querystring": "^0.2.0",
		"react-router-dom": "^5.2.0",
		"react-select": "^3.1.0",
