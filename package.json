	"devDependencies": {
		"@babel/preset-es2017": "^7.0.0-beta.53",
		"@fortawesome/fontawesome-free": "^5.14.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.31",
		"@fortawesome/free-brands-svg-icons": "^5.15.1",
		"@fortawesome/free-regular-svg-icons": "^5.14.0",
